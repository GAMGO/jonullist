{
  "expo": {
    "plugins": [
      "expo-secure-store",
      "expo-localization"
    ],
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "산책 거리 측정을 위해 위치를 사용합니다."
      }
    }
  }
}
